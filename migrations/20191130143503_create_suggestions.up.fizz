create_table("suggestions") {
	t.Column("id", "uuid", {primary: true})
	t.Column("title", "string", {})
	t.Column("body", "string", {})
	t.Column("dev_comment", "string", {null: true})
	t.Column("version_when_suggested", "string", {})
	t.Column("upvoted_by", "map[string]user", {})
	t.Column("downvoted_by", "map[string]user", {})
	t.Column("editable", "bool", {})

	t.Timestamps()
}