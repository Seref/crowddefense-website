create_table("suggestions") {
	t.Column("id", "uuid", {primary: true})
	t.Column("title", "string", {})
	t.Column("body", "text", {})
	t.Column("dev_comment", "string", {null: true})
	t.Column("version_when_suggested", "string", {null: true})
	t.Column("upvoted_by", "json", {})
	t.Column("downvoted_by", "json", {})
	t.Column("editable", "bool", {})
	t.Column("keywords", "json", {})
	t.Column("fulfilled", "bool", {})
	t.Column("suggested_by", "string", {})
	t.Timestamps()
}